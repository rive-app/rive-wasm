    "Umberto Sonnino <umberto@rive.app> (https://rive.app)",
    "Matthew Sullivan <matt.j.sullivan@gmail.com> (mailto:matt.j.sullivan@gmail.com)"
  ],
  "license": "MIT",
  "files": [
    "dist/rive.min.js",
    "dist/rive.min.js.map",
    "dist/rive_single.js",
    "dist/rive_single.js.map",
    "dist/rive_light.js",
    "dist/rive_light.js.map",
    "dist/rive.js",
    "dist/rive.js.map",
    "dist/rive_canvas.wasm",
    "dist/rive_canvas_light.mjs",
    "dist/rive_canvas_single.mjs",
    "dist/rive_canvas.mjs",
    "src/rive.ts",
    "src/rive_canvas.mjs.d.ts",
    "dist/rive.d.ts"
  ],
  "typings": "./dist/rive.d.ts",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@types/offscreencanvas": "^2019.7.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "auto-changelog": "^2.4.0",
    "babel-jest": "^27.0.2",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prettier": "^4.2.1",
    "filemanager-webpack-plugin": "^8.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "prettier-eslint": "^15.0.1",
    "release-it": "^14.14.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.10.0"
  }
