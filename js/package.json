  },
  "scripts": {
    "dev": "npx webpack watch",
    "release": "release-it",
    "changelog": "auto-changelog -p --output ../CHANGELOG.md --commit-limit false --template keepachangelog --sort-commits date-desc --starting-version 1.1.1 --hide-credit",
    "release:patch": "npm run release -- --ci",
    "release:minor": "npm run release -- minor --ci",
    "release:major": "npm run release -- major --ci",
    "lint": "eslint ./src --fix",
    "build": "npx webpack",
    "build:all": "./build.sh",
    "bumpVersions": "./bump_all_versions.sh $npm_package_version",
    "publish:all": "./publish_all.sh --access public",
    "minify": "npx terser --compress --mangle -o ./dist/rive.js -- ./src/rive.js",
    "test": "jest"
  },
  "keywords": [
    "rive",
    "animation"
  ],
  "author": "Rive",
  "contributors": [
    "Luigi Rosso <luigi@rive.app> (https://rive.app)",
